{
  "name": "action-cond",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haya14busa/action-cond.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "haya14busa",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^5.42.1",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^8.27.0",
    "eslint-plugin-github": "^4.4.1",
    "eslint-plugin-jest": "^27.1.4",
    "jest": "^29.3.1",
    "jest-circus": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "^16.13.1"
  }
}
